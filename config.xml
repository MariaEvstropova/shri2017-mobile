<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.mariaevstropova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>feedme</name>
    <description>
        FeedMe will find recipes by provided products list
    </description>
    <author email="m.evstropova@yandex.ru" href="https://github.com/MariaEvstropova">
        Maria Evstropova
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="21" />
    <preference name="SplashScreen" value="www/res/android/drawable/screen.png" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <platform name="android">
        <icon density="ldpi" src="www/res/android/drawable-ldpi/icon.png" />
        <icon density="mdpi" src="www/res/android/drawable-mdpi/icon.png" />
        <icon density="hdpi" src="www/res/android/drawable-hdpi/icon.png" />
        <icon density="xhdpi" src="www/res/android/drawable-xhdpi/icon.png" />
        <icon density="xxhdpi" src="www/res/android/drawable-xxhdpi/icon.png" />
        <icon density="xxxhdpi" src="www/res/android/drawable-xxxhdpi/icon.png" />
        <splash density="land-ldpi" src="www/res/android/drawable-land-ldpi/screen.png" />
        <splash density="land-mdpi" src="www/res/android/drawable-land-mdpi/screen.png" />
        <splash density="land-hdpi" src="www/res/android/drawable-land-hdpi/screen.png" />
        <splash density="land-xhdpi" src="www/res/android/drawable-land-xhdpi/screen.png" />
        <splash density="land-xxhdpi" src="www/res/android/drawable-land-xxhdpi/screen.png" />
        <splash density="land-xxhdpi" src="www/res/android/drawable-land-xxxhdpi/screen.png" />
        <splash density="port-ldpi" src="www/res/android/drawable-ldpi/screen.png" />
        <splash density="port-mdpi" src="www/res/android/drawable-mdpi/screen.png" />
        <splash density="port-hdpi" src="www/res/android/drawable-hdpi/screen.png" />
        <splash density="port-xhdpi" src="www/res/android/drawable-xhdpi/screen.png" />
        <splash density="port-xxhdpi" src="www/res/android/drawable-xxhdpi/screen.png" />
        <splash density="port-xxhdpi" src="www/res/android/drawable-xxxhdpi/screen.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="android" spec="~6.2.3" />
    <plugin name="com.phonegap.plugins.barcodescanner" spec="https://github.com/Telerik-Verified-Plugins/BarcodeScanner" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
    <plugin name="nl.x-services.plugins.socialsharing" spec="https://github.com/Telerik-Verified-Plugins/SocialSharing" />
</widget>
